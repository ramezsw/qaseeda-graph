#!/home/ramez/python/capstone/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'py2neo==2.0.5','console_scripts','cypher'
__requires__ = 'py2neo==2.0.5'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('py2neo==2.0.5', 'console_scripts', 'cypher')()
    )
