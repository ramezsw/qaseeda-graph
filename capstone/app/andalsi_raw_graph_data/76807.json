{"id": "76807", "author": "أبو الحسن الششتري", "abyat": [{"bayt": " دارَت عليك الاقْداح      بِرَوحٍ ورَاحْ\r", "sadr": " دارَت عليك الاقْداح", "ajez": "      بِرَوحٍ ورَاحْ\r"}, {"bayt": " فعجْ على الْخَمَّار   بخلع العذار\r", "sadr": " فعجْ على الْخَمَّار", "ajez": "   بخلع العذار\r"}, {"bayt": " تُنْصِر سنا الأنْوار     إِذا ما تُدار\r", "sadr": " تُنْصِر سنا الأنْوار", "ajez": "     إِذا ما تُدار\r"}, {"bayt": " وعالَمُ الأسْرارْ     يَلُحْ لَكَ جِهَارْ\r", "sadr": " وعالَمُ الأسْرارْ", "ajez": "     يَلُحْ لَكَ جِهَارْ\r"}, {"bayt": " والرَّاح روحُ الأرواح      ما فيها جُناحْ\r", "sadr": " والرَّاح روحُ الأرواح", "ajez": "      ما فيها جُناحْ\r"}, {"bayt": " دارَت عليْكَ الأقْداح     برَوحْ ورَاح\r", "sadr": " دارَت عليْكَ الأقْداح", "ajez": "     برَوحْ ورَاح\r"}, {"bayt": " جمالها مشهورْ   في الدِّين القديم\r", "sadr": " جمالها مشهورْ", "ajez": "   في الدِّين القديم\r"}, {"bayt": " لاَحت ولاَحَ النُّور    في اللَّيل البهيمْ\r", "sadr": " لاَحت ولاَحَ النُّور", "ajez": "    في اللَّيل البهيمْ\r"}, {"bayt": " ودُكَّ مِنْها الطُّور   لموسى الكليم\r", "sadr": " ودُكَّ مِنْها الطُّور", "ajez": "   لموسى الكليم\r"}, {"bayt": " وحين الْقى الألْواح    بالمكتوم باحْ\r", "sadr": " وحين الْقى الألْواح", "ajez": "    بالمكتوم باحْ\r"}, {"bayt": " دارَت عليك الأقداح     برَوْح وراح\r", "sadr": " دارَت عليك الأقداح", "ajez": "     برَوْح وراح\r"}, {"bayt": " جاءت بأنس النَّفْس      والسِّحْرِ الحلال\r", "sadr": " جاءت بأنس النَّفْس", "ajez": "      والسِّحْرِ الحلال\r"}, {"bayt": " نزِّهَت عن جِنْس   جَلَّت عن مثال\r", "sadr": " نزِّهَت عن جِنْس", "ajez": "   جَلَّت عن مثال\r"}, {"bayt": " وأشْرَقَت كالشَّمس     في أفِقْ الجمال\r", "sadr": " وأشْرَقَت كالشَّمس", "ajez": "     في أفِقْ الجمال\r"}, {"bayt": " وبَشَّرت بالأفْراح   لأَهْلِ الفلاح\r", "sadr": " وبَشَّرت بالأفْراح", "ajez": "   لأَهْلِ الفلاح\r"}, {"bayt": " دارَت عليْكَ الاقْداح       بِرَوْحٍ وراحْ\r", "sadr": " دارَت عليْكَ الاقْداح", "ajez": "       بِرَوْحٍ وراحْ\r"}, {"bayt": " يا كلَّ كلِّ الكل    جُدْ لِ